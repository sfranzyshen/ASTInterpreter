  "code": "// Nested Loops and Variable Scope\nvoid setup() {\n  Serial.begin(9600);\n}\n\nvoid loop() {\n  int i = 0;\n  while (i < 3) {\n    for (int j = 0; j < 3; j++) {\n      int k = 0;\n      do {\n        Serial.print(\"i: \");\n        Serial.print(i);\n        Serial.print(\", j: \");\n        Serial.print(j);\n        Serial.print(\", k: \");\n        Serial.println(k);\n        k++;\n      } while (k < 2);\n    }\n    i++;\n  }\n}",
