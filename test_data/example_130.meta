content=// Bit-Fields in Structs
struct MyFlags {
  unsigned int a : 1;
  unsigned int b : 1;
  unsigned int c : 6;
};

void setup() {
  Serial.begin(9600);
}

void loop() {
  struct MyFlags flags;
  flags.a = 1;
  flags.b = 0;
  flags.c = 50;

  Serial.print("Flag a: ");
  Serial.println(flags.a);
  Serial.print("Flag c: ");
  Serial.println(flags.c);
}