content=// Array Indexing and Multidimensional Arrays
void setup() {
  Serial.begin(9600);
}

void loop() {
  int arr[5] = {10, 20, 30, 40, 50};
  int matrix[2][3] = {{1, 2, 3}, {4, 5, 6}};
  int i = 2;
  int j = 1;
  int k = 1;

  int val1 = arr[i * 2 - 1];
  int val2 = matrix[j][k];

  Serial.print("Val 1: ");
  Serial.println(val1);
  Serial.print("Val 2: ");
  Serial.println(val2);
}