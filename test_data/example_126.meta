content=// Self-Referential Structs (Linked List Node)
struct Node {
  int data;
  struct Node* next;
};

void setup() {
  Serial.begin(9600);
}

void loop() {
  struct Node n1, n2;
  n1.data = 10;
  n1.next = &n2;

  n2.data = 20;
  n2.next = NULL;

  Serial.print("Data from n1: ");
  Serial.println(n1.data);
  Serial.print("Data from n2 via n1: ");
  Serial.println(n1.next->data);
}