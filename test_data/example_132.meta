content=// Ternary Operator with Complex Expressions
int getBigger(int x, int y) {
  return (x > y) ? x : y;
}

void setup() {
  Serial.begin(9600);
}

void loop() {
  int a = 10, b = 20, c = 30;

  int result = (a < b) ? getBigger(a, c) : (a + b);

  Serial.print("Final result: ");
  Serial.println(result);
}