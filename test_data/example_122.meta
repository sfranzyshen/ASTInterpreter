void setup() {
  Serial.begin(9600);
}

void loop() {
  int a = 10;
  char b = 'B';
  float c = 3.14;

  Serial.print("Size of int: ");
  Serial.println(sizeof(a));

  Serial.print("Size of char: ");
  Serial.println(sizeof(char));

  Serial.print("Size of float: ");
  Serial.println(sizeof(c));
}