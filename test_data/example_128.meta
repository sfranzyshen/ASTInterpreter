content=// Unsigned Integer Rollover
void setup() {
  Serial.begin(9600);
}

void loop() {
  unsigned int i = 4294967295;
  Serial.print("Initial value: ");
  Serial.println(i);
  i++;
  Serial.print("After increment (rollover): ");
  Serial.println(i);
  i = 0;
  i--;
  Serial.print("After decrement (rollover): ");
  Serial.println(i);
}