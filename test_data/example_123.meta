content=// Comma Operator in a for Loop
void setup() {
  Serial.begin(9600);
}

void loop() {
  int a = 0, b = 10;
  for (int i = 0; i < 3; i++) {
    Serial.print("Initial a: ");
    Serial.println(a);
    a = (a++, b++);
    Serial.print("Final a: ");
    Serial.println(a);
  }
}