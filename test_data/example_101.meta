void setup() {
  Serial.begin(9600);
}

void loop() {
  int x = 10; int y = 20; if (x < y) { Serial.println("X is less than Y."); }
  int i = 0; while (i < 3) { Serial.println(i); i++; }
  for (int j = 0; j < 2; j++) { Serial.print("J is: "); Serial.println(j); }
}