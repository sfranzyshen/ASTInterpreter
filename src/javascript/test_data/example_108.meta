content=// Complex for loop with multiple expressions
void setup() {
  Serial.begin(9600);
}

void loop() {
  int i = 0;
  int j = 10;

  for (i = 0, j = 10; i < j; i++, j--) {
    Serial.print("i: ");
    Serial.print(i);
    Serial.print(", j: ");
    Serial.print(j);
    Serial.println(j);
  }
}