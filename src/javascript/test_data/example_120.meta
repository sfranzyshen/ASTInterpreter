content=// Conditional Operator in a Loop
void setup() {
  Serial.begin(9600);
}

void loop() {
  int x = 0;
  for (int i = 0; i < 5; i++) {
    x = (i == 0) ? 1 : ((i % 2 == 0) ? i + 10 : i * 10);
    Serial.print("For i=");
    Serial.print(i);
    Serial.print(", x is: ");
    Serial.println(x);
  }
}