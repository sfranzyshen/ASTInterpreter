content=// Function Pointers and Callbacks
int myFunc(int a, int b) {
  return a + b;
}

void callFunc(int (*funcPtr)(int, int)) {
  Serial.print("Result: ");
  Serial.println(funcPtr(10, 20));
}

void setup() {
  Serial.begin(9600);
}

void loop() {
  int (*ptr)(int, int);
  ptr = &myFunc;
  callFunc(ptr);
}