content=// static Global Variable and Function
static int global_counter = 0;

static void incrementCounter() {
  global_counter++;
}

void setup() {
  Serial.begin(9600);
}

void loop() {
  incrementCounter();
  Serial.print("Counter: ");
  Serial.println(global_counter);
  delay(1000);
}