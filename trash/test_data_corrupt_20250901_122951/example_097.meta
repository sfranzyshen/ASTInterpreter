content=// Loop Control Statements (break, continue)
void setup() {
  Serial.begin(9600);
}

void loop() {
  for (int i = 0; i < 5; i++) {
    for (int j = 0; j < 5; j++) {
      if (j == 2) {
        Serial.println("Continuing inner loop...");
        continue;
      }
      if (i == 3) {
        Serial.println("Breaking outer loop...");
        break;
      }
      Serial.print("i: ");
      Serial.print(i);
      Serial.print(", j: ");
      Serial.print(j);
      Serial.println(j);
    }
  }
}