content=// Nested for and if with break and continue
void setup() {
  Serial.begin(9600);
}

void loop() {
  for (int i = 0; i < 5; i++) {
    for (int j = 0; j < 5; j++) {
      if (i == 2 && j == 2) {
        Serial.println("Skipping inner loop...");
        continue;
      }
      if (i == 3) {
        Serial.println("Breaking outer loop...");
        break;
      }
      Serial.print("i: ");
      Serial.print(i);
      Serial.print(", j: ");
      Serial.print(j);
      Serial.println(j);
    }
    if (i == 3) {
      break;
    }
  }
}