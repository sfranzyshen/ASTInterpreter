void setup() {
  Serial.begin(9600);
}

void loop() {
  char myChar = 'A';
  char escapedChar = '\n';
  const char* str1 = "Hello";
  const char* str2 = " world!";

  Serial.print(str1);
  Serial.println(str2);
  Serial.print("My character is: ");
  Serial.println(myChar);

  Serial.print(myChar);
  Serial.print(escapedChar);
}