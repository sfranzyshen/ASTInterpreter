const int optoPin = 2;

void setup() {
  // make the pin with the optocoupler an output
  pinMode(optoPin, OUTPUT);
}

void loop() {
  digitalWrite(optoPin, HIGH);
  delay(15);
  digitalWrite(optoPin, LOW);
  delay(10000);
}
