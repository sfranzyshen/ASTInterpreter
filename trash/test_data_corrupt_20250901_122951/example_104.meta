content=// Complex Preprocessor Directives
#define PI 3.14159
#define CIRCLE_AREA(r) (PI * r * r)

void setup() {
  Serial.begin(9600);
}

void loop() {
  #ifdef PI
    float radius = 5.0;
    float area = CIRCLE_AREA(radius);
    Serial.print("Area of circle: ");
    Serial.println(area);
  #else
    Serial.println("PI is not defined.");
  #endif
}